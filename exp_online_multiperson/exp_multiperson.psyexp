        <Param val="" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="if ((((parID % 2) === 0) &amp;&amp; (nTextRun === 1))) {&amp;#10;    textToPre = text1_1;&amp;#10;} else {&amp;#10;    if ((((parID % 2) === 1) &amp;&amp; (nTextRun === 1))) {&amp;#10;        textToPre = text2_1;&amp;#10;    } else {&amp;#10;        if ((((parID % 2) === 0) &amp;&amp; (nTextRun === 2))) {&amp;#10;            textToPre = text1_2;&amp;#10;        } else {&amp;#10;            if ((((parID % 2) === 1) &amp;&amp; (nTextRun === 2))) {&amp;#10;                textToPre = text2_2;&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;textToPre = textToPre.split(&quot; &quot;);&amp;#10;nWords = textToPre.length;&amp;#10;nScreens = (Number.parseInt(((nWords - 1) / 10)) + 1);&amp;#10;currentScreen = 0;&amp;#10;tChange = (tThisFlipGlobal + 3.34);&amp;#10;startWord = 0;&amp;#10;textsOnScreen = textToPre.slice(startWord, (startWord + 10)).join(&quot; &quot;);&amp;#10;tEnd = 9999999999999999999;&amp;#10;resetBool = [];&amp;#10;for (var i, _pj_c = 0, _pj_a = util.range((nScreens + 1)), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    i = _pj_a[_pj_c];&amp;#10;    resetBool.push(0);&amp;#10;}&amp;#10;resetBool[currentScreen] = 1;&amp;#10;currentScreen = (currentScreen + 1);&amp;#10;thisFrame = 0;&amp;#10;slider_data = [];&amp;#10;continuousSlider_2.markerPos = 50;&amp;#10;mouseRec = mouse.getPos();&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="# select a trial sequence based on sub ID&amp;#10;if parID%2 == 0 and nTextRun == 1:&amp;#10;    textToPre = text1_1&amp;#10;elif parID%2 == 1 and nTextRun == 1:&amp;#10;    textToPre = text2_1&amp;#10;elif parID%2 == 0 and nTextRun == 2:&amp;#10;    textToPre = text1_2&amp;#10;elif parID%2 == 1 and nTextRun == 2:&amp;#10;    textToPre = text2_2&amp;#10;textToPre = textToPre.split()&amp;#10;nWords = len(textToPre)&amp;#10;nScreens = int((nWords-1)/10)+1    # number of screens needed&amp;#10;currentScreen = 0    # which screen we are at now&amp;#10;&amp;#10;# get the time for the first flip&amp;#10;tChange = tThisFlipGlobal + 3.34   # add 7 ms lag&amp;#10;startWord = 0    # location of the first word to present&amp;#10;textsOnScreen = &quot; &quot;.join(textToPre[startWord:startWord+10])&amp;#10;tEnd = 9999999999999999999    # time to end&amp;#10;&amp;#10;# monitor whether each screen has been reset&amp;#10;resetBool = []&amp;#10;for i in range(nScreens+1):&amp;#10;    resetBool.append(0)&amp;#10;resetBool[currentScreen] = 1&amp;#10;currentScreen = currentScreen + 1&amp;#10;&amp;#10;# frame counter&amp;#10;thisFrame = 0&amp;#10;&amp;#10;# create an empty list to store data&amp;#10;slider_data=[]&amp;#10;&amp;#10;# set start position of the slider to the mid point&amp;#10;continuousSlider_2.markerPos = 50&amp;#10;&amp;#10;# record the current position of the mouse&amp;#10;mouseRec=mouse.getPos()" valType="extendedCode" updates="constant" name="Begin Routine"/>
